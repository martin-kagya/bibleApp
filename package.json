{
  "name": "bible-presentation-app",
  "version": "2.0.0",
  "description": "Comprehensive AI-powered presentation software for preachers with real-time speech recognition and semantic scripture suggestions",
  "main": "electron.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server.js",
    "server": "nodemon server.js",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "start:dev": "concurrently -n \"VITE,ELECTRON\" -c \"blue,yellow\" \"npm run dev\" \"wait-on http://localhost:3000 && npm run electron\"",
    "embeddings:generate": "electron scripts/generateEmbeddings.js",
    "test": "node tests/run-all-tests.js",
    "test:paraphrase": "node tests/paraphrase-detection.test.js",
    "test:parsing": "node tests/reference-parsing.test.js",
    "test:sermon": "node test-preacher-sermon.js",
    "electron": "electron .",
    "electron:build": "electron-builder",
    "build:all": "npm run build && npm run electron:build"
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^2.0.0",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.6.0",
    "better-sqlite3": "^12.5.0",
    "chromadb": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "form-data": "^4.0.0",
    "fs-extra": "^11.3.2",
    "ioredis": "^5.3.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "openai": "^4.28.0",
